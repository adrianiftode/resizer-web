- data_pixel_width = 640
- normal_width = 240
- quality_high = 90
- quality_low = 80
- image_prefix = "http://z.zr.io/rw/contracts/"
- feature = [["world-code.jpg", "Custom Development", "10 Hours Custom Development", "Custom Development by the people that know ImageResizer best. If you are looking for the best performance, practices and security for ImageResizer, this the service for you."], ["touchscreen-architecture.jpg", "5 hours Architecture Planning", "5 hours Architecture Planning", "Whether you at the beginning stages of your project or updating an existing one, consult with the ImageResizer experts and resolve issues upfront."], ["data-center.jpg", "10 Assisted Installations", "10 Assisted Installations", "Save time by allowing us to do up to 10 assisted installs per year. We know ImageResizer best and use our experience and best practices to ensure things are running smoothly."], ["clock-tech-closeup.jpg", "Guaranteed Support Response time in 2 hours", "Guaranteed Support Response time in 2 hours", "Our enterprise customers don't have to wait. Get a response from our support staff within two hours, guaranteed."], ["speedometer.jpg", "Priority Hot-fixes in 2 Business Days, Guaranteed", "Priority Hot-fixes in 2 Business Days, Guaranteed", "If you are running enterprise level applications it is critical that everything is running as expected. With our Gold Plan we guarantee priority hot-fixes wihtin two business days."], ["support-button.jpg", "Unlimited Support Incidents", "Unlimited Support Incidents", "Where some support contracts limited you to one support incident during business hours, with the Gold Plan you get an unlimited number."], ["email.jpg", "Unlimited Designated Contacts", "Unlimited Designated Contacts", "Unlike the Bronze Plan you get an unlimited number of contacts at your organization. That means anyone from your company can contact us for support."], ["code-lock.jpg", "Custom e-mail to contact support", "Custom e-mail to contact support", "When you sign up for this support contract we will create a custom email address that you can use to get priority support."], ["touchscreen-gears.jpg", "Free Plugin Upgrades", "Free Plugin Upgrades", "Keep your app secure and up to date with free plugin upgrades for the duration of your contract."]]

- feature.each_slice(4) do | f_b |
	.row
		- f_b.each do | f |
			.span3
				p
					img alt="f[1]" data_pixel_width="#{data_pixel_width}" data-slimmage="true" data-src="#{image_prefix}#{f[0]}?width=#{normal_width}&format=jpg&quality=#{quality_high}" src="#{image_prefix}#{f[0]}?width=#{data_pixel_width}&format=jpg&quality=#{quality_low}"
				h3 #{f[2]}
				p #{f[3]}
	.row
		.span12
			p &nbsp;



